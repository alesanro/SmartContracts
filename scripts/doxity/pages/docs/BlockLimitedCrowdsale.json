\n    }\n\n    Params public config;\n\n    function BlockLimitedCrowdsale(address _serviceProvider, bytes32 _symbol, address _priceTicker)\n              ChronoMintCrowdsale(_serviceProvider, _symbol, _priceTicker)\n    {\n    }\n\n    function init(\n        bytes32 _currencyCode,\n        uint _minValue,\n        uint _maxValue,\n        uint _exchangeRate,\n        uint _exchangeRateDecimals,\n        uint256 _startBlock,\n        uint256 _stopBlock\n    ) onlyAuthorised onlyOnce {\n        require (_stopBlock > _startBlock);\n        require (block.number < _stopBlock);\n\n        init(_currencyCode, _minValue, _maxValue, _exchangeRate, _exchangeRateDecimals);\n\n        config.startBlock = _startBlock;\n        config.stopBlock = _stopBlock;\n    }\n\n    function isRunning() constant returns (bool) {\n        return block.number > config.startBlock\n                  && block.number < config.stopBlock\n                  && GenericCrowdsale.isRunning();\n    }\n\n    function isFailed() constant returns (bool) {\n        return block.number > config.stopBlock\n                && !GenericCrowdsale.isSuccessed();\n    }\n}\n","abiDocs":[{"constant":true,"inputs":[],"name":"getSymbol","payable":false,"stateMutability":"view","type":"function","notice":"Returns token's symbol;","signature":"getSymbol()","signatureHash":"15070401"},{"constant":true,"inputs":[],"name":"isRunning","payable":false,"stateMutability":"view","type":"function","signature":"isRunning()","signatureHash":"2014e5d1"},{"constant":true,"inputs":[{"name":"","type":"bytes32"}],"name":"raised","payable":false,"stateMutability":"view","type":"function","signature":"raised(bytes32)","signatureHash":"2bac85ad"},{"constant":false,"inputs":[{"name":"_investor","type":"address"},{"name":"_currencyCode","type":"bytes32"}],"name":"refund","payable":false,"stateMutability":"nonpayable","type":"function","notice":"Refunds donated fund.","signature":"refund(address,bytes32)","signatureHash":"37b6ee7e"},{"con